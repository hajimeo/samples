{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "description": "Extension to open links in Salesforce Lightning.",
   "icons": {
      "128": "click-to-console-logo.png"
   },
   "manifest_version": 2,
   "name": "Click-to-Console",
   "options_page": "options.html",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "http://*/", "https://*/", "webRequest", "webNavigation", "cookies", "storage" ],
   "version": "0.9.0"
}
