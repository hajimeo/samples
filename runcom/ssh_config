Host *
  ServerAliveInterval 20

Host 10.* 172.* 192.168.* *.localdomain node* localhost
  #ForwardAgent yes
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel QUIET

Host *.cloudapp.net
  ServerAliveInterval 20
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host dh1
  Hostname dockerhost1
  User hosako

Host asftp
  Hostname sftp.atscale.com

Host node* atscale* *.localdomain localhost
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  #IdentityFile /path/to/root/id_rsa
  User root